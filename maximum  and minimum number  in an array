//maximum  and minimum number  in an array
#include <stdio.h>
int main() {
int n;
printf("enter the size :");
scanf("%d",&n);
    int a[n];
    int max=0;
    for (int i=0;i<n;i++){
        scanf("%d",&a[i]);
        if(a[i]>max){
            max=a[i];
        }  }printf("max num :%d\n",max);
        for (int i=0;i<n;i++){
    if (a[i]<max){
            max=a[i];
        }}
    printf("small num :%d",max);
}
