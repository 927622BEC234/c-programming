#include<stdio.h>
int main()
{   int n;
printf("enitr the month 1-12 :");
    scanf("%d",&n);
    if(n==1 || n==3 || n==5 ||n==7 || n==8 || n==10 ||n==12)
    {printf("31 days ,month %d",n);}
    else if (n==4||n==6||n==9||n==11)
    {printf("30 days ,month %d",n);}   
    else if (n==2)
    {printf("28 or 29 days,month %d",n);
    }
    else  
    {printf("invalied input %d",n);}
}
