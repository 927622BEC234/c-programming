#include<stdio.h>
int main(){
    int a;
    printf("enter an number :");
    scanf("%d",&a);
    if (a%5==0 && a%7==0){
        printf("the number is divisible by both 5 and 7:%d",a);}
        else if (a%5==0)
        {printf("the number is divisible by 5 :%d",a);}
        else if (a%7==0)
        {
            printf("the number is divisable by 7 :%d",a);}
            else 
            {printf ("the number is not divisible by 5 and 7  :%d",a);}
    return 0;
}
